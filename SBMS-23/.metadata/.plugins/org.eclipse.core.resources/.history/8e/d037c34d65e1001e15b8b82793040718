package com.uj.methodreference;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;

public class MethodRef {
	
	public static void m2() {
		for(int i = 1; i<=10;i++) {
			System.out.println(i);
		}
	}

	public static void main(String[] args) {
//		MyInterface mi = MethodRef::m2;
//		mi.m1();
		
		String[ ]  names = {"Anushka", "Anupama", "Deepika", "Kajol", "Sunny" };
		String[ ] newString = null;
		for(int i=0;  i<names.length; i++) {
			if(names[i].charAt(0) == 'A') {
				System.out.println(names[i]);
			}
		}
		
		List<String> nameList = Arrays.asList(names);
		List<String> newList = nameList.stream().filter(s -> s.startsWith(s, 'A')).collect(Collectors.toList());
		System.out.println(newList.toString());
		
		
	}
}


