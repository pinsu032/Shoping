package com.uj.methodreference;

public class MethodRef {
	
	public static void m2() {
		for(int i = 1; i<=10;i++) {
			System.out.println(i);
		}
	}

	public static void main(String[] args) {
		MyInterface mi = MethodRef::m2;
		mi.m1();
		
		
	}
}

@FunctionalInterface
interface MyInterface{
	void m1();
}
