package com.uj.methodreference;

public class MethodRef  extends Thread{
	
	@Override
	public void run() {
		
		for(int i=1;i<=10;i++) {
			System.out.println(i);
		}
		
	}

	public static void main(String[] args) {
		MethodRef obj = new MethodRef();
		Thread t1 = new Thread(obj); //new 
		t1.start();
	}
}


