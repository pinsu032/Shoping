package com.uj.stream;

import java.util.Arrays;
import java.util.List;
import java.util.regex.Pattern;

public class StreamMethodDemo {

	public static void main(String[] args) {
		List<String> password = Arrays.asList("india@","usa#","uk@", "japan#");
		String regExp = "[^a-zA-Z0-9 ]";
		Pattern pattern = Pattern.compile(regExp);
		//allMatch();
		System.out.println("india@".matches(regExp));
		boolean allMatch = password.stream().allMatch(p -> p.matches(regExp));
		
		if(allMatch) {
			System.out.println("All String contain special character..");
		}
		else {
			System.out.println("Some String does'nt have special character");
		}
	}

}
